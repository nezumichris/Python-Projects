with open(r'C:\Users\Nezum\Downloads\AdventofCodeDay1.txt', 'r') as file:
    alist = [] #initialize list
    blist = []
    for line in file:
        a, b = map(int, line.split()) 
        alist.append(a)
        blist.append(b)

alist.sort() 
blist.sort()

i = 0
m = 0

while i < len(alist):
    x = alist[i]
    y = blist[i]
    z = abs(x - y)
    m += z
    i += 1

aDict = list(dict.fromkeys(alist))
#print(aDict)

i = 0
sumStuff = 0 
while i < len(alist):
    x = alist[i]
    y = blist.count(x)
    multStuff = x * y
    print(x,y)
    print(multStuff)
    sumStuff += multStuff 
    i += 1
print(sumStuff)

